{
  "name": "better-auth-ts",
  "version": "0.1.0",
  "description": "better-auth",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "server": "jiti src/examples/server.ts",
    "test": "vitest run src/tests/api.test.ts src/tests/token.test.ts",
    "test:run": "vitest run --reporter=verbose src/tests/api.test.ts src/tests/token.test.ts",
    "test:watch": "vitest --watch src/tests/api.test.ts src/tests/token.test.ts",
    "test:coverage": "vitest run --coverage src/tests/api.test.ts src/tests/token.test.ts",
    "test:integration": "vitest run src/tests/integration.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check"
  },
  "keywords": [
    "authentication",
    "security",
    "cryptography"
  ],
  "author": "Jason Colburne",
  "license": "MIT",
  "devDependencies": {
    "@noble/hashes": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/pako": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.5.1",
    "pako": "^2.1.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
